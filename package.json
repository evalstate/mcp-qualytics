{
  "name": "@llmindset/mcp-qualytics",
  "displayName": "MCP Qualytics",
  "description": "A Code Metric Analysis MCP Server",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "mcp-qualytics": "./dist/server.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evalstate/mcp-qualytics.git"
  },
  "bugs": {
    "url": "https://github.com/evalstate/mcp-qualytics/issues"
  },
  "homepage": "https://github.com/evalstate/mcp-qualytics#readme",
  "scripts": {
    "prebuild": "npm test",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "start": "node ./dist/server.js",
    "prepublishOnly": "npm run build",
    "postbuild": "node -e \"process.platform !== 'win32' && require('child_process').execSync('chmod +x dist/server.js')\"",
    "test:generate": "node --loader ts-node/esm test/golden-master.ts generate",
    "test:verify": "node --loader ts-node/esm test/golden-master.ts verify",
    "test": "npm run test:verify"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@typescript-eslint/typescript-estree": "^5.0.0",
    "estraverse": "^5.3.0",
    "express": "^4.18.2",
    "ignore": "^7.0.3",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/estraverse": "^5.1.7",
    "@types/express": "^4.17.21",
    "@types/node": "^14.14.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}